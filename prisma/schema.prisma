// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id Int @id
  cant_jugadas Int @default(0)
}

model Jugada {
  id Int @id // chat id del usuario
  jugada String
  enviar String
}


